generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Animal {
  scientific_name String  @id
  common_name     String?
  latitude        Float?
  longitude       Float?
  status          String?
  class           String?
  family          String?
  genus           String?
  kingdom         String?
  order           String?
  phylum          String?
  type            String?
  image           String?
}
